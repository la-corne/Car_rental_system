<p id="notice"><%= notice %></p>

<h1>Saved Cars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_cars.each do |saved_car| %>
      <tr>
        <td><%= link_to 'Show', saved_car %></td>
        <td><%= link_to 'Edit', edit_saved_car_path(saved_car) %></td>
        <td><%= link_to 'Destroy', saved_car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved Car', new_saved_car_path %>
