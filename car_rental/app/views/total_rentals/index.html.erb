<p id="notice"><%= notice %></p>

<h1>Total Rentals</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @total_rentals.each do |total_rental| %>
      <tr>
        <td><%= link_to 'Show', total_rental %></td>
        <td><%= link_to 'Edit', edit_total_rental_path(total_rental) %></td>
        <td><%= link_to 'Destroy', total_rental, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Total Rental', new_total_rental_path %>
